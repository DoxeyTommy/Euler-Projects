package euler;

import java.math.BigInteger;

public class main {
	
	public static void main(String[] args) {
		
		int count = 0;
		for (int n = 1; n <= 9; n++) 
		{
			for (int i = 1; i <= 21; i++) 
			{
				if (BigInteger.valueOf(n).pow(i).toString().length() == i)
					count++;
			}
		}
		System.out.println(Integer.toString(count));
	}
		 
	//solution: 49

}
