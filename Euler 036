package euler;

public class main {

	public static void main(String[] args) {
		
		int sum = 0;
		for (int i = 1; i < 1000000; i++) 
		{
			if (Library.isPalindrome(Integer.toString(i, 10)) && Library.isPalindrome(Integer.toString(i, 2)))
				sum += i;
		}
		System.out.println(Integer.toString(sum));
	}
		 
	//solution: 872187
}
